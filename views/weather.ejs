<%- include("header.ejs") %>
<div class="container">
  <h1>Weather App</h1>
  <form action="/" method="post">
  <input id =" cityInput" type="text" name="cityName" placeholder="Enter City Name..." required>
  <button type="submit">Search</button>
  </form>
</div>
<div class="card">
  <div class="card-top">
    <img src="<%= img %>" >
  </div>
  <div class="card-content">
    <h2>  <%=query %></h2>
    <p> <%= desc %></p>
    <p> <%= temp %> &deg; C</p>
  </div>
</div>

<%- include("footer.ejs") %>